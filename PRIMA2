#include<iostream>
using namespace std;
int main (){
	int angkaAwal = 1;
	int angkaAkhir = 100;
	
	for ( int i = angkaAwal ; i <= angkaAkhir ; i++ ){
		int counterPrima = 0;
		
		for (int k = 1 ; k <= i ; k++){
			if ( i % k == 0 ){
				counterPrima++;
			}
			
		}
		if( counterPrima == 2){
			cout<< i << " adalah bilangan Prima"<<endl;
		}
		
	}
}
