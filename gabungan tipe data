#include<iostream>
using namespace std;
int main(){
	string nama;
	int umur;
	float tinggi;
	double kebugaranTubuh;
	char jenis_kelamin;
	
	
	cout << "siapakah namamu" << endl;
	cout << "jawab: ";
	getline(cin,nama);
	
	cout << "berapa umurmu" << endl;
	cout << "jawab: ";
	cin >> umur;
	
	cout << "berapa tinggi" << endl;
	cout << "jawab: ";
	cin >> tinggi;
	
	cout << "bagaimana kondisi kebugaran tubuhmu" << endl;
	cout << "jawab: ";
	cin >> kebugaranTubuh;
	
	cout << "jenis kelamin [L/P]: ";
	cin >> jenis_kelamin;
	
	cout << "salam kenal, " << nama << " sekarang engkau berusia "<< endl;
	cout << umur << " tahun "<< endl;
	cout << tinggi << " tinggi saya "<< endl;
	cout << kebugaranTubuh << " % kebugaran tubuh saya "<< endl;
	cout << "dan saya berjenis kelamin "<<jenis_kelamin;
	
	return 0;
}
